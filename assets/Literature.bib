	journal = {Neural Comput. Appl.},
	month = {Mar.},
	number = {8},
	pages = {3983--3996},
	title = {Explainable empirical risk minimization},
	volume = {36},
	year = {2024},
	bdsk-url-1 = {https://doi.org/10.1007/s00521-023-09269-3},
