	isbn = {1530-0366},
	journal = {Genetics in Medicine},
	number = {7},
	pages = {743--750},
	title = {The Precision Medicine Initiative's All of Us Research Program: an agenda for research on its ethical, legal, and social issues},
	url = {https://doi.org/10.1038/gim.2016.183},
	volume = {19},
	year = {2017},
