	journal = {British Journal of Educational Psychology},
	number = {3},
	pages = {535-551},
	title = {Influences on cognitive engagement: Epistemological beliefs and need for closure},
	url = {https://bpspsychub.onlinelibrary.wiley.com/doi/abs/10.1348/000709905X53138},
	volume = {76},
