	keywords = {radio transmitters;signal detection;software radio;RF-sensing;activity detection;radio frequency channel;software defined radio;radio transmitter;one-stage classifier;two-stage classifier;local signals;ambient signals;device-free recognition systems;noncooperative subjects;Sensors;Accuracy;Monitoring;Radio transmitters;Wireless communication;Transceivers;Feature extraction;Pervasive computing;signal analysis;synthesis;and processing;signal processing;location-dependent and sensitive},
	month = {April},
