	doi = {10.1126/science.aat8084},
	eprint = {https://www.science.org/doi/pdf/10.1126/science.aat8084},
	journal = {Science},
	number = {6406},
	pages = {1004-1008},
	title = {All-optical machine learning using diffractive deep neural networks},
	url = {https://www.science.org/doi/abs/10.1126/science.aat8084},
	volume = {361},
	year = {2018},
	bdsk-url-1 = {https://www.science.org/doi/abs/10.1126/science.aat8084},
