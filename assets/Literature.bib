	id = {Munn2020},
	isbn = {2662-9992},
	journal = {Humanities and Social Sciences Communications},
	number = {1},
	pages = {53},
	title = {Angry by design: toxic communication and technical architectures},
	url = {https://doi.org/10.1057/s41599-020-00550-7},
	volume = {7},
