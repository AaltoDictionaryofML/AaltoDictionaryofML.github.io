	id = {Freiesleben2023},
	isbn = {1573-0964},
	journal = {Synthese},
	number = {4},
	pages = {109},
	title = {Beyond generalization: a theory of robustness in machine learning},
	url = {https://doi.org/10.1007/s11229-023-04334-9},
	volume = {202},
	year = {2023},
	bdsk-url-1 = {https://doi.org/10.1007/s11229-023-04334-9}}

